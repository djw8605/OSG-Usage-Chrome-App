{
  "manifest_version": 2,
  "name": "OSG Graph Saver",
  "description": "OSG Graph Saver",
  "version": "0.1.0",
  "permissions": [
      "tabs",
      "http://gratiaweb.grid.iu.edu/gratia/*",
      "https://osg-gratia-share.appspot.com/"
  ],
  "content_scripts": [
      {
        "matches": ["http://gratiaweb.grid.iu.edu/gratia/*"],
        "css": ["content.css"],
        "js": ["lib/jquery-1.11.0.min.js", "content_script.js"]
      }
    ],
  
  "icons": { "16": "osglogo.16.png"}
}
